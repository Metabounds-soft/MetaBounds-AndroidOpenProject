<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension" />
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension" />
        <attr name="indicatorName" format="string" />
        <attr name="indicatorColor" format="color" />
    </declare-styleable>

    <declare-styleable name="VisionTextView">
        <attr name="draw_start_width" format="dimension|reference" />
        <attr name="draw_start_height" format="dimension|reference" />
        <attr name="draw_end_width" format="dimension|reference" />
        <attr name="draw_end_height" format="dimension|reference" />
        <attr name="draw_top_width" format="dimension|reference" />
        <attr name="draw_top_height" format="dimension|reference" />
        <attr name="use_ping_fang_type_face" format="boolean" />
        <attr name="is_bold" format="boolean" />
    </declare-styleable>

</resources>